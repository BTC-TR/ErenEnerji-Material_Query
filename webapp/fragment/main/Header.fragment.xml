<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:f="sap.ui.layout.form"
    xmlns:l="sap.ui.layout"
>
    <f:SimpleForm
        id="_IDGenSimpleForm1"
        editable="false"
        layout="ColumnLayout"
    >
        <Label
            id="_IDGenLabel112"
            text="{i18n>LgplaDesc}"
        />
        <SearchField
                  value="{viewModel>/LgplaSearch}"
            id="idAddress"
            placeholder="{i18n>LgplaDescPH}"
            search="onLgplaCheck"
            maxLength="13"
        >
            <layoutData>
                <l:GridData
                    id="_IDGenGridData123"
                    span="XL4 L4 M4 S6"
                />
            </layoutData>
        </SearchField>
        <Label
            id="_IDGenLabel1"
            text="{i18n>Barcode}"
        />
        <SearchField
            id="idBarcode"
            placeholder="{i18n>okutunuz}"
            search="onChangeBarcode"
         
        >
            <layoutData>
                <l:GridData
                    id="_IDGenGridData1"
                    span="XL4 L4 M4 S6"
                />
            </layoutData>
        </SearchField>
        <Label
            id="_IDGenLabel12"
            text="{i18n>Matnr}"
        />
        <Text
            id="_IDGenText1"
            text="{viewModel>/FormInfo/EvMaktx}"
        >
            <layoutData>
                <l:GridData
                    id="_IDGenGridData12"
                    span="XL4 L4 M4 S6"
                />
            </layoutData>
        </Text>
        <!-- <Label id="_IDGenLabel13" text="{i18n>Maktx}"/> -->
        <Text
            id="_IDGenText2"
            text="{viewModel>/FormInfo/Maktx}"
        >
            <layoutData>
                <l:GridData
                    id="_IDGenGridData13"
                    span="XL4 L4 M4 S6"
                />
            </layoutData>
        </Text>
        <!-- <Label id="_IDGenLabel4" text="{i18n>Charg}"/>
			<Text id="_IDGenInput1" text="{viewModel>/Charg}" >
				<layoutData>
					<l:GridData id="_IDGenGridData6" span="XL4 L4 M4 S6"/>
				</layoutData>
			</Text> -->
    </f:SimpleForm>
</core:FragmentDefinition>
